<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
<session-factory>
 
 <property name="hibernate.connection.driver_class">

com.mysql.jdbc.Driver</property>

  <property name="hibernate.connection.url">

jdbc:mysql://localhost/youthmedia</property>

  <property name="hibernate.connection.username">root</property>
  <property name="hibernate.connection.password">root</property>
  <property name="hibernate.connection.pool_size">100</property>
  
  <property name="show_sql">true</property>
  <property name="dialect">org.hibernate.dialect.MySQLDialect</property>
  <property name="hibernate.hbm2ddl.auto">update</property>
  <!-- Mapping files -->
  <mapping resource="vo/categoryvo.hbm.xml"/>
  <mapping resource="vo/subCategoryVO.hbm.xml"/>
  <mapping resource="vo/FileUploadVO.hbm.xml"/>
  <mapping resource="vo/RegistrationVO.hbm.xml"/>
  <mapping resource="vo/LoginVO.hbm.xml"/>
  <mapping resource="vo/CityVO.hbm.xml"/>
  <mapping resource="vo/CountryVO.hbm.xml"/>
  <mapping resource="vo/StateVO.hbm.xml"/>
  <mapping resource="vo/CommentVO.hbm.xml"/>
   <mapping resource="vo/FeedbackVO.hbm.xml"/>
   <mapping resource="vo/ComplainVO.hbm.xml"/>
  
     
</session-factory>
</hibernate-configuration>